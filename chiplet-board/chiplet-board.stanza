public defn molex-lr-box-sym (n:Int, name:Symbol|False) :
  val ps = PinSpec(
    expand-range("p", 0, n / 2 - 1)
    expand-range("p", n / 2, n - 1)
  )
  match(name) :
    (s:Symbol) :
      generic-box-sym(ps, s)
    (f) :
      generic-box-sym(ps)



