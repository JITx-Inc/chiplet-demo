  inst pc-0v8 : {power-conditioning(0.8, 3.0)}

  net - (pc-3v3.dst, pc-1v8.pow3v3, pc-0v9.pow3v3, pc-0v8.pow3v3)
  net sup3v3 (power-con.p[9], pc-3v3.src.pos)
  net sup1v8 (power-con.p[5], power-con.p[6] pc-1v8.src.pos)
