  inst ls : level-shifter[2]
  
  net - (ls[0].pwr-hv, ls[1].pwr-hv, pc-3v3.dst.pos)
  net - (ls[0].pwr-lv, ls[1].pwr-lv, pc-1v8.dst.pos)
