;==== Board ====================================================================

defn make-chips-board () :
  
  pcb-board B :
    stackup = tempo-automation-10-layer-62-mil
    boundary = brd-outline 
    signal-boundary = brd-outline 

  make-board(chips) :
    board = B
    rules = tempo-automation-adv-rules

;==== Schematic ================================================================

defn make-chips-schematic () :
  make-schematic(chips)

;==== Project ==================================================================

defn make-chips-project () :
  make-chips-schematic()
  make-chips-board()

make-chips-project()

